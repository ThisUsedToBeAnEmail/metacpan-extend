{
	"name": "meta::cpan::extend",
	"version": "1.0",
	"description": "meta::cpan as an extension",
	"permissions": ["activeTab", "notifications", "declarativeContent", "storage", "https://fastapi.metacpan.org/" ],
	"options_page": "options.html",
	"background": {
		"scripts": ["terse.js", "model.js", "background.js"],
		"persistent": false
	},
	"browser_action": {
	"default_popup": "popup.html",
	"default_icon": {
		"16": "images/meta-16.png",
		"32": "images/meta-32.png",
		"48": "images/meta-48.png",
		"128": "images/meta-128.png"
	}
	},
	"icons": {
		"16": "images/meta-16.png",
		"32": "images/meta-32.png",
		"48": "images/meta-48.png",
		"128": "images/meta-128.png"
	},
	"web_accessible_resources": [
		"images/metacpan-logo.png",
		"images/meta-48.png"
	],
	"manifest_version": 2
}
